{
    "name": "proteo5/sld",
    "description": "Single Line Data (SLD) and Multi Line Data (MLD) format parser and encoder for PHP",
    "version": "1.1.0",
    "type": "library",
    "keywords": [
        "sld",
        "mld",
        "data-format",
        "parser",
        "encoder",
        "serialization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "SLD Project Contributors",
            "homepage": "https://github.com/proteo5/sld"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SLD\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SLD\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "homepage": "https://github.com/proteo5/sld",
    "support": {
        "issues": "https://github.com/proteo5/sld/issues",
        "source": "https://github.com/proteo5/sld"
    }
}
